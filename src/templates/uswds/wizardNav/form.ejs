<ul class="padding-0 usa-list--unstyled display-flex" id="{{ ctx.wizardKey }}-nav">
  {% if (ctx.buttons.cancel) { %}
  <li>
    <button class="usa-button usa-button--secondary btn-wizard-nav-cancel" ref="{{ctx.wizardKey}}-cancel">{{ctx.t('cancel')}}</button>
  </li>
  {% } %}
  {% if (ctx.buttons.previous) { %}
  <li>
    <button class="usa-button btn-wizard-nav-previous" ref="{{ctx.wizardKey}}-previous">{{ctx.t('previous')}}</button>
  </li>
  {% } %}
  {% if (ctx.buttons.next) { %}
  <li>
    <button class="usa-button btn-wizard-nav-next" ref="{{ctx.wizardKey}}-next">{{ctx.t('next')}}</button>
  </li>
  {% } %}
  {% if (ctx.buttons.submit) { %}
  <li>
    <button class="usa-button btn-wizard-nav-submit" ref="{{ctx.wizardKey}}-submit">{{ctx.t('submit')}}</button>
  </li>
  {% } %}
</ul>
