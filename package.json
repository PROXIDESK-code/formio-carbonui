    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formio/uswds.git"
  },
  "keywords": [
    "Form.io",
    "USWDS",
    "Us",
    "Web",
    "Design",
    "Standards"
