  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/ejs": "^2.6.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.2",
    "@types/sinon": "^7.5.0",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-template": "^5.0.0",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "sinon": "^7.5.0",
    "ts-node": "^8.4.1",
    "ts-sinon": "^1.0.24",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
