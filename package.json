    "@types/sinon": "^9.0.8",
    "chai": "^4.2.0",
    "del": "^6.0.0",
    "formiojs": "^4.12.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^2.0.0",
